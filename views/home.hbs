<div class="container-fluid paddding mb-5">
    <div class="row mx-0">
        <div class="col-md-6 col-12 paddding animate-box card-article"
            onclick="window.location='/articles/detail/{{articleMostOutstanding_1.id}}'" data-animate-effect="fadeIn">
            <div class="fh5co_suceefh5co_height img-hover">
                <div class="fh5co_suceefh5co_height_position_absolute img-hover"></div>
                <img src="/public{{articleMostOutstanding_1.small_avt}}" alt="img" />
                <div class="fh5co_suceefh5co_height_position_absolute_font">
                    <div class="">
                        <a href="/articles/detail/{{articleMostOutstanding_1.id}}" class="color_fff">
                            <i class="fa fa-clock-o"></i>
                            &nbsp;&nbsp;Dec 31,2017
                        </a>
                    </div>
                    <div class="">
                        <a href="/articles/detail/{{articleMostOutstanding_1.id}}" class="fh5co_good_font">
                            {{articleMostOutstanding_1.title}}
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="row">
                {{#each this.articleMostOutstanding_4}}
                <div class="col-md-6 col-6 paddding animate-box card-article"
                    onclick="window.location='/articles/detail/{{id}}'" data-animate-effect="fadeIn">
                    <div class="fh5co_suceefh5co_height_2 img-hover">
                        <div class="fh5co_suceefh5co_height_position_absolute"></div>
                        <img src="/public{{small_avt}}" alt="img" />
                        <div class="fh5co_suceefh5co_height_position_absolute_font_2">
                            <div class="">
                                <a href="/articles/detail/{{id}}" class="color_fff"> <i
                                        class="fa fa-clock-o"></i>&nbsp;&nbsp;Oct
                                    28,2017 </a>
                            </div>
                            <div class="">
                                <a href="/articles/detail/{{id}}" class="fh5co_good_font_2">
                                    {{title}}
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>
<div class="container-fluid pt-3">
    <div class="container animate-box" data-animate-effect="fadeIn">
        <div>
            <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">Trending</div>
        </div>
        <div class="owl-carousel owl-theme js d-flex" id="slider1">
            {{#each this.articleMostView_10}}
            <div class="item px-2 card-article" onclick="window.location='/articles/detail/{{id}}'">
                <div class="fh5co_latest_trading_img_position_relative">
                    <div class="fh5co_latest_trading_img"><img src="/public{{small_avt}}" alt=""
                            class="fh5co_img_special_relative" /></div>
                    <div class="fh5co_latest_trading_img_position_absolute"></div>
                    <div class="fh5co_latest_trading_img_position_absolute_1">
                        <a href="/articles/detail/{{id}}" class="text-white"
                            style="font-size: 16px; font-weight: bold;">
                            {{title}}
                        </a>
                        <div class="fh5co_latest_trading_date_and_name_color b-0"> Walter Johson - March 7,2017</div>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>
<div class="container-fluid pb-4 pt-5">
    <div class="container animate-box">
        <div>
            <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">News</div>
        </div>
        <div class="owl-carousel owl-theme card-article" id="slider2">
            {{#each this.articleNew_10}}
            <div class="item px-2 h-100" onclick="window.location='/articles/detail/{{id}}'">
                <div class="fh5co_hover_news_img">
                    <div class="fh5co_news_img"><img src="/public{{small_avt}}" alt="" /></div>
                    <div class="d-flex flex-column justify-content-between">
                        <a href="/articles/detail/{{id}}" class="d-block fh5co_small_post_heading">
                            <span style="font-size: 16px; font-weight: bold;">
                                {{title}}
                            </span>
                        </a>
                        <div class="c_g"><i class="fa fa-clock-o"></i> Oct 16,2017</div>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>

